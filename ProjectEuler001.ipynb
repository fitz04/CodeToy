   "source": [
    "# 1000보다 작은 자연수 중에서 3 또는 5의 배수를 모두 더하면?\n",
    "res = 0\n",
    "for i in range(1000):\n",
    "\tif (i%3 is 0) or (i%5 is 0):\n",
    "\t\tres += i\n",
    "print (res)\n",
    "\n",
    "print(sum([x for x in range(1000) if x % 3 == 0 or x % 5 == 0]))\n",
    "\n",
    "print(sum(filter(lambda x: x%3 == 0 or x%5 == 0, range(1000)) ))"
   ]
  }
